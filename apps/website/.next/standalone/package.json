{
  "name": "saas-startup-platform",
  "version": "0.1.0",
  "private": true,
  "description": "SaaS & Tech Startup platform with public website and CMS",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:containers": "docker compose up --build",
    "dev:containers:optimized": "docker compose -f docker-compose.yml -f docker-compose.build.yml up --build",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "test:ui": "turbo test:storybook",
    "test:visual": "turbo test:visual",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules/.cache",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "containers:up": "docker compose up -d",
    "containers:down": "docker compose down",
    "containers:logs": "docker compose logs -f",
    "containers:build": "docker compose build",
    "containers:build:optimized": "docker compose -f docker-compose.yml -f docker-compose.build.yml build",
    "containers:build:parallel": "docker compose -f docker-compose.yml -f docker-compose.build.yml build --parallel",
    "containers:build:monitored": "scripts/docker-build-monitored.sh compose",
    "containers:build:monitored:all": "scripts/docker-build-monitored.sh all",
    "containers:clean": "docker compose down -v --remove-orphans",
    "containers:prod": "docker compose -f docker-compose.prod.yml up -d",
    "containers:prod:build": "docker compose -f docker-compose.prod.yml build",
    "db:init": "node scripts/init-db.js",
    "db:reset": "node scripts/reset-db.js",
    "db:seed": "node scripts/seed-db.js",
    "db:migrate": "node scripts/migrate-db.js run",
    "db:migrate:list": "node scripts/migrate-db.js list",
    "db:test:init": "node scripts/init-test-db.js",
    "db:test:clean": "node scripts/init-test-db.js clean",
    "db:test": "node scripts/test-scripts.js",
    "setup": "pnpm install && pnpm containers:build",
    "reset": "pnpm clean && pnpm containers:clean && pnpm setup",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:integration": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh",
    "test:integration:api": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh api",
    "test:integration:load": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh load",
    "test:integration:container": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh container",
    "test:integration:security": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh security",
    "test:integration:responsive": "chmod +x scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh responsive",
    "test:api": "node tests/integration/api-endpoints.test.js",
    "test:load": "node tests/load/backend-load-test.js",
    "test:security": "node tests/integration/security-validation.test.js",
    "test:containers": "node tests/integration/container-orchestration.test.js",
    "test:docker": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "test:docker:unit": "docker compose -f docker-compose.test.yml up --build website-test cms-test --abort-on-container-exit",
    "test:docker:integration": "docker compose -f docker-compose.test.yml up --build integration-test --abort-on-container-exit",
    "test:docker:e2e": "docker compose -f docker-compose.test.yml up --build e2e-test --abort-on-container-exit",
    "test:docker:clean": "docker compose -f docker-compose.test.yml down -v --remove-orphans",
    "build:monitor": "node scripts/build-performance-monitor.js",
    "build:monitor:service": "node scripts/build-performance-monitor.js service",
    "build:alerts": "node scripts/build-alerts.js check",
    "build:alerts:trends": "node scripts/build-alerts.js trends",
    "build:alerts:history": "node scripts/build-alerts.js history",
    "build:metrics:export": "node scripts/build-metrics-exporter.js export",
    "build:metrics:serve": "node scripts/build-metrics-exporter.js serve",
    "build:metrics:test": "node scripts/build-metrics-exporter.js test",
    "perf:runtime": "node scripts/runtime-performance-monitor.js full",
    "perf:runtime:pageload": "node scripts/runtime-performance-monitor.js pageload",
    "perf:runtime:resources": "node scripts/runtime-performance-monitor.js resources",
    "perf:regression": "node scripts/performance-regression-test.js test",
    "perf:baseline": "node scripts/performance-regression-test.js baseline show",
    "perf:baseline:update": "node scripts/performance-regression-test.js baseline update",
    "perf:test": "scripts/performance-test-workflow.sh full",
    "perf:test:build": "scripts/performance-test-workflow.sh build",
    "perf:test:runtime": "scripts/performance-test-workflow.sh runtime",
    "perf:test:regression": "scripts/performance-test-workflow.sh regression",
    "perf:report": "scripts/performance-test-workflow.sh report",
    "perf:summary": "scripts/performance-test-workflow.sh summary",
    "health:check": "scripts/health-check.sh",
    "health:monitor": "scripts/continuous-health-monitor.sh",
    "backup:create": "scripts/create-backup.sh create",
    "backup:list": "scripts/create-backup.sh list",
    "backup:restore": "scripts/create-backup.sh restore",
    "rollback:auto": "scripts/rollback.sh auto",
    "rollback:config": "scripts/rollback.sh manual config",
    "rollback:images": "scripts/rollback.sh manual images",
    "rollback:code": "scripts/rollback.sh manual code",
    "rollback:full": "scripts/rollback.sh manual full",
    "emergency:stop": "scripts/emergency-recovery.sh stop",
    "emergency:recover": "scripts/emergency-recovery.sh recover",
    "emergency:rebuild": "scripts/emergency-recovery.sh rebuild",
    "emergency:status": "scripts/emergency-recovery.sh status"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.11.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "esbuild": ">=0.25.0"
    }
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.15.0"
  }
}